{
  "version": 2,
  "buildCommand": "npm install && npx prisma generate && npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "build": {
    "env": {
      "PRISMA_ENGINES_CHECKSUM_IGNORE_MISSING": "1"
    }
  },
  "functions": {
    "api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"]
}
